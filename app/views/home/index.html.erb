<%= image_tag('SpendIt.png') %>
  <div>
  </div>
  <div>
    <ul class="login-bar">
      <% if user_signed_in? %>
        <li>
          <%= button_to "Sign Out", destroy_user_session_path, class: "sign-btn", method: :delete %>
        </li>
      <% else %>
        <li>
          <%= button_to "Sign In", new_user_session_path, class: "sign-btn" %>
          <%= link_to "Sign Up", new_user_registration_path, class: "sign-btn" %>
        </li>
      <% end %>
    </ul>
  </div>